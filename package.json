    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.23.0",
    "dotenv": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^1.1.5",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
